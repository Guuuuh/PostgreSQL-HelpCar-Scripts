CREATE TABLE public.EMPRESA_ENDERECO
(
	CODEMPRESA_ENDERECO		INTEGER NOT NULL,
	CODEMPRESA				INTEGER NOT NULL,
	CODENDERECO				INTEGER NOT NULL,
	CONSTRAINT PK_CODEMPRESA_ENDERECO PRIMARY KEY (CODEMPRESA_ENDERECO),
	CONSTRAINT FK_CODENDERECO2 FOREIGN KEY (CODENDERECO)
	REFERENCES PUBLIC.ENDERECO (CODENDERECO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT 
);