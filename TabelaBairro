CREATE TABLE public.BAIRRO
(
    CODBAIRRO 		INTEGER   NOT NULL,
    NOME 			VARCHAR(100) NOT NULL,
	CODCIDADE		INTEGER NOT NULL,
    CONSTRAINT PK_CODBAIRRO PRIMARY KEY (CODBAIRRO),
	CONSTRAINT FK_CODCIDADE FOREIGN KEY (CODCIDADE)
	REFERENCES PUBLIC.CIDADE (CODCIDADE) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT
);