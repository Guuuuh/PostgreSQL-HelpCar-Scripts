CREATE TABLE public.CLIENTE_ENDERECO
(
	CODCLIENTE_ENDERECO			INTEGER NOT NULL,
	CODENDERECO					INTEGER NOT NULL,
	CODCLIENTE					INTEGER NOT NULL,
	CONSTRAINT PK_CODCLIEND PRIMARY KEY (CODCLIEND),
	CONSTRAINT FK_CODENDERECO FOREIGN KEY (CODENDERECO)
	REFERENCES PUBLIC.ENDERECO (CODENDERECO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT,
	CONSTRAINT FK_CODCLIENTE FOREIGN KEY (CODCLIENTE)
	REFERENCES PUBLIC.CLIENTE (CODCLIENTE) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT
);