CREATE TABLE public.ENDERECO
(
    CODENDERECO 	INTEGER   NOT NULL,
    CEP 			VARCHAR(11) NOT NULL,
	RUA				VARCHAR(250) NOT NULL,
	NUMERO			VARCHAR(10) NOT NULL,
	CODBAIRRO		INTEGER NOT NULL,
    CONSTRAINT PK_CODENDERECO PRIMARY KEY (CODENDERECO),
	CONSTRAINT FK_CODBAIRRO FOREIGN KEY (CODBAIRRO)
	REFERENCES PUBLIC.BAIRRO (CODBAIRRO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT
);