CREATE TABLE public.CLIENTESERVICO
(
	CODCLIENTESERVICO		INTEGER NOT NULL,
	CODEMPRESA				INTEGER NOT NULL,
	CODSERVICO				INTEGER NOT NULL,
	CODVEICULO				INTEGER NOT NULL,
	CODCLIENTE				INTEGER NOT NULL,
	CONSTRAINT PK_CODCLIENTESERVICO PRIMARY KEY (CODCLIENTESERVICO),
	CONSTRAINT FK_CODEMPRESA FOREIGN KEY (CODEMPRESA)
	REFERENCES PUBLIC.EMPRESA (CODEMPRESA) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT,
	CONSTRAINT FK_CODSERVICO FOREIGN KEY (CODSERVICO)
	REFERENCES PUBLIC.SERVICO (CODSERVICO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT,
	CONSTRAINT FK_CODVEICULO FOREIGN KEY (CODVEICULO)
	REFERENCES PUBLIC.VEICULO (CODVEICULO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT,
	CONSTRAINT FK_CODCLIENTE FOREIGN KEY (CODCLIENTE)
	REFERENCES PUBLIC.CLIENTE (CODCLIENTE) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT
);