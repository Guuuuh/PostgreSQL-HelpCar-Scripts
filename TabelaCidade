CREATE TABLE public.CIDADE
(
    CODCIDADE 		INTEGER   NOT NULL,
    NOME 			VARCHAR(100)NOT NULL,
	CODESTADO		INTEGER NOT NULL,
    CONSTRAINT PK_CODCIDADE PRIMARY KEY (CODCIDADE),
	CONSTRAINT FK_CODESTADO FOREIGN KEY (CODESTADO)
	REFERENCES PUBLIC.ESTADO (CODESTADO) MATCH SIMPLE
	ON UPDATE CASCADE
	ON DELETE RESTRICT
);